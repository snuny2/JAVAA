package w0919;

import java.util.Scanner;

public class simWhile {

	public static void main(String[] args) {
		int num, nu;

		Scanner s = new Scanner(System.in);
		System.out.print("반복할 횟수를 입력하시길.. : ");
		nu = s.nextInt();
		num = 0;
		
		do {
			System.out.println("While문을 이용하여 반복합니다.");
			num++;
		}while(num<nu);
	}

}
