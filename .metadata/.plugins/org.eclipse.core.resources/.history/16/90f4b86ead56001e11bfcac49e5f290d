package w0919;

import java.util.Scanner;

public class forrr110 {

	public static void main(String[] args) {
		int num = 0;
		int nu;
		int nn;
		
		Scanner s = new Scanner(System.in);
		nu = s.nextInt();

		for(nn=1;nn<nu;nn++) {
			nu += nn;
		}
		System.out.print("1부터 10까지의 합은 "+num+" 입니다..");

	}

}
